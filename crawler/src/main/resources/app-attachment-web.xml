<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<entry key="attachment-temp-directory">C:/upload-directory/attachment-temp-directory</entry>
	<entry key="attachment-formal-directory">C:/upload-directory/attachment-formal-directory</entry>
	<entry key="attachment-kindeditor-directory">C:/upload-directory/attachment-kindeditor-directory</entry>
